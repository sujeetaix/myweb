pipeline {
      agent any
      
       stages {
           stage ('checkout') {
           steps {
                 git credentialsId: 'GIT_HUB_CREDENTIALS', url: 'https://github.com/sujeetaix/myweb.git'
                 }
            }
               
              
    stage("building") {
           steps{ 
                def mvnHome = tool name:'mvn', type: 'mvn'
                def mvnCMD = "${mvnHome}/bin/mvn"
                sh "${mvnCMD} clean package"
                sh "mv target/*.war target/myweb.war"
    }
}
}
}
