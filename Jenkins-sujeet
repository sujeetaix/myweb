pipeline {
      agent any
      
       stages {
           stage ('checkout') {
           steps {
                 git credentialsId: 'GIT_HUB_CREDENTIALS', url: 'https://github.com/sujeetaix/myweb.git'
                 }
            }
               
              
    stage("building") {
           steps{ 
                export MAVEN_HOME=/opt/apache-maven-3.8.2/
                sh "mvn --version"
                sh "mvn clean package"
                }
}
}
}
